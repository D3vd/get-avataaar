{
  "version": 2,
  "name": "get-avataaar",
  "alias": ["get-avataaar.now.sh"],
  "builds": [
    {
      "src": "api/**/*",
      "use": "@now/go"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ]
}
